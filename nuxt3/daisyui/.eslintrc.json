{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "standard-with-typescript",
    "plugin:tailwindcss/recommended"
  ],
  "overrides": [
  ],
  "ignorePatterns": ["tailwind.config.ts", "nuxt.config.ts"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["."]
  },
  "plugins": [
    "vue",
    "tailwindcss",
    "import",
    "unused-imports"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "unused-imports/no-unused-imports": "warn",
    "quotes": ["error", "double"]
  }
}